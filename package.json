{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && rm -rf docs && mv dist docs",
    "distribute": "vue-cli-service serve --mode development",
    "update": "yarn build && git add . && git commit -m 'update' && git push"
  },
  "dependencies": {
    "@types/node": "^14.14.33",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuetify": "^2.4.6"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^1.13.5",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "prettier": "prettier/prettier#cd2b8a32b5ef77eb0047e7517b1ea22d605a094a",
    "sass": "^1.32.8",
    "sass-loader": "10.1.1",
    "typescript": "^4.2.3",
    "vue-template-compiler": "^2.6.12"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "prettier": {
    "pluginSearchDirs": [
      "."
    ],
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
